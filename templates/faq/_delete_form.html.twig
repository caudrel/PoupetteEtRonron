<form method="post" action="{{ path('app_faq_delete', {'id': faq.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette FAQ ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ faq.id) }}">
    <button class="btn pr-button-orange">Supprimer</button>
</form>